<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <Color x:Key="Dark-Blueberry">#1a202a</Color>
    <Color x:Key="Semi-Dark-Blueberry">#2a3549</Color>
    <Color x:Key="Blueberry">#3f5374</Color>
    <Color x:Key="Light-Blueberry">#95acca</Color>
    <Color x:Key="Light">#f5f8fd</Color>


    <SolidColorBrush x:Key="Light-Brush" Color="{DynamicResource Light}"/>
    <SolidColorBrush x:Key="Light-Blueberry-Brush" Color="{DynamicResource Light-Blueberry}"/>
    <SolidColorBrush x:Key="Blueberry-Brush" Color="{DynamicResource Blueberry}"/>
    <SolidColorBrush x:Key="Semi-Dark-Blueberry-Brush" Color="{DynamicResource Semi-Dark-Blueberry}"/>
    <SolidColorBrush x:Key="Dark-Blueberry-Brush" Color="{DynamicResource Dark-Blueberry}"/>

</ResourceDictionary>